<h1>Login</h1>
<%= form_with scope: :session, url: login_path do |form| %>
    <p>
        <%= form.label "Email: " %>
        <%= form.text_field :email %>
    </p>
    <p>
        <%= form.label "Password: " %>
        <%= form.password_field :password %>
    </p>
    <p>
        <%= form.submit "Login", data: {turbo: false} %>
    </p>
<% end %>